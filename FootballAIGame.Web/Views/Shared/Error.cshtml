@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h2 class="text-danger">Error</h2>

<h3 style="margin-top: 1em" class="text-danger">An unexpected error occurred while processing your request.</h3>


<h4 style="margin-top: 2em">Error details:</h4>

<p>
     @Model.Exception
</p>